{
    "name": "sTels logistic technology",
    "version": "1.0.2",
    "manifest_version": 3,
    "description": "This app will help you parse data and create card in trello",
    "permissions": [
        "tabs",
        "notifications",
        "contextMenus",
        "activeTab",
        "storage",
        "sessions",
        "identity",
        "scripting"
    ],
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "64": "icon.png",
        "128": "icon.png",
        "256": "icon.png"
    },
    "action": {
        "default_title": "Title",
        "default_popup": "popup.html"
    }
}