{
    "name": "sTels logistic technology",
    "version": "0.2",
    "manifest_version": 3,
    "description": "This app will help you parse data and create string",
    "permissions": ["storage", "tabs", "activeTab"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "event.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_title": "Title",
        "default_popup": "popup.html"
    }
}